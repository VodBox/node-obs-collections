{
  "name": "obs-collections",
  "version": "1.0.0",
  "description": "JavaScript library for interacting with and modifying OBS Studio scene collections",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "scripts": {
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir esm --target es2018",
    "build": "npm run build:cjs && npm run build:es",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VodBox/node-obs-collections.git"
  },
  "author": "VodBox (Dillon Pentz)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VodBox/node-obs-collections/issues"
  },
  "homepage": "https://github.com/VodBox/node-obs-collections#readme",
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@types/node": "^18.19.118",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  }
}
